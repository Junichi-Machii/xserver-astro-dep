---
// library imports
import { Icon } from "astro-icon";

import Link from "./Link.astro";

//data imports
import  navData  from "../data/navData";
import MobilNav from "./MobilNav.astro";

---

<nav aria-label="Primary">
  <div class="navbar">
    <a href="/" class="logomark" aria-label="Go home">
      <Icon name="logomark" width="60" />
      <h1 class="text-xs">JUNICHI MACHII</h1>
    </a>
      <ul>
        {
          navData.map((item) => (
            <Link
              text={item.name}
              href={item.path}
              style="secondary"
              isFilled={false}
              data-navLink
              icon={{
                name: "mdi:account-plus",
                side: "left",
              }}
            />
          ))
        }
      </ul>
      
   <MobilNav


  </div>
</nav>

<script>
  import "../js/nav.js";
</script>